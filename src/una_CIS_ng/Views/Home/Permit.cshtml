            <li ng-show="!permit.permits.row.parties.holder.isInfrastructureOwner"><a href="#PermitApplicationInfOwner">Infrastructure Owner</a></li>
          </ul>
        </li>
        <li>
          <a href="#PermitApplicationLocation">Location</a>
          <ul class="nav nav-stacked">
            <li><a href="#PermitApplicationMap">Map</a></li>
            <li><a href="#PermitApplicationLocationCalculation">Details</a></li>
          </ul>
        </li>
        <li>
          <a href="#PermitApplicationFeeEstimation">Fees Estimate</a>
        </li>
        <li>
          <a href="#PermitApplicationSubmit">Submit</a>
        </li>
      </ul>

    </nav>
  </div>

